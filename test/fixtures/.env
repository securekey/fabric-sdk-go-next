# This file contains the default images and tags used in the docker-based
# Fabric fixtures. The images and tags can be overridden using environment
# variables. See docker compose documentation.

# Default architecture
ARCH=x86_64

CORE_PEER_NETWORKID=fabsdkgo
COMPOSE_PROJECT_NAME=fabsdkgo

FABRIC_CA_FIXTURE_TAG=1.0.4
FABRIC_ORDERER_FIXTURE_TAG=latest
FABRIC_PEER_FIXTURE_TAG=latest
FABRIC_COUCHDB_FIXTURE_TAG=latest
FABRIC_BUILDER_FIXTURE_TAG=latest
FABRIC_BASEOS_FIXTURE_TAG=latest
FABRIC_BASEIMAGE_FIXTURE_TAG=0.4.2

FABRIC_CA_FIXTURE_IMAGE=hyperledger/fabric-ca
FABRIC_ORDERER_FIXTURE_IMAGE=repo.onetap.ca:8443/next/hyperledger/fabric-orderer
FABRIC_PEER_FIXTURE_IMAGE=repo.onetap.ca:8443/next/hyperledger/fabric-peer
FABRIC_COUCHDB_FIXTURE_IMAGE=repo.onetap.ca:8443/next/hyperledger/fabric-couchdb
FABRIC_BUILDER_FIXTURE_IMAGE=repo.onetap.ca:8443/next/hyperledger/fabric-ccenv
FABRIC_BASEOS_FIXTURE_IMAGE=repo.onetap.ca:8443/next/hyperledger/fabric-baseos
FABRIC_BASEIMAGE_FIXTURE_IMAGE=hyperledger/fabric-baseimage
